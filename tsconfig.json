{
	"extends": "@react-native/typescript-config/tsconfig.json",
	"compilerOptions": {
		"jsx": "react",
		"baseUrl": "./src",
		"module": "esnext",
		"paths": {
			"@api": ["./api"],
			"@assets/*": ["./assets/*"],
			"@features/*": ["./features/*"],
			"@environment/*": ["./environment/*"],
			"@navigations": ["./navigations"],
			"@navigations/*": ["./navigations/*"],
			"@store/*": ["./store/*"],
			"@hooks/*": ["./shared/hooks/*"],
			"@components/*": ["./shared/components/*"],
			"@services/*": ["./shared/services/*"],
			"@utils/*": ["./shared/utils/*"],
			"@providers/*": ["./shared/providers/*"],
			"@environment": ["./environment"]
		}
	},
	"include": ["src","types.d.ts"]
}
